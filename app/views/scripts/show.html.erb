<p id="notice"><%= notice %></p>
<p id="alert"><%= alert %></p>

<p>
  <strong>Name:</strong>
  <%= @script.name %>
</p>

<p>
  <strong>Description:</strong>
  <%= @script.description %>
</p>

<p>
  <strong>Script:</strong>
  <%= simple_format @script.body %>
</p>

<p>
  <strong>Passed?:</strong>
  <%= @script.passed %>
</p>

<%= link_to 'Edit', edit_script_path(@script) %> |
<%= link_to 'Back', scripts_path %>

<p>
  <%= image_tag('/screenshot.png', width: 700) %>
</p>

